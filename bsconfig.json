{
  "name": "bisect-starter-bsb-jest",
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "bs-dependencies": [
    "bisect_ppx"
  ],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest"
  ],
  "ppx-flags": [
    ["bisect_ppx/ppx", "--exclude-files", ".*\\/__tests__\\/.*\\.re$$"]
  ],
  "refmt": 3,
  "sources": [
    ".",
    { "dir": "__tests__", "type": "dev" }
  ]
}
